{
  "name": "hubsite-api",
  "scripts": {
    "build": "tsup src/server.ts --format cjs",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server.ts",
    "start-watch": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "ts-node src/server.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.4",
    "@prisma/client": "^4.3.1",
    "apollo-server": "^3.10.0",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "awesome-phonenumber": "^3.3.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "uuid": "^9.0.0",
    "typedi": "0.10.0",
    "hubsite-models": "*"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/node": "14.14.11",
    "@types/uuid": "^8.3.4",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "2.0.0",
    "tsup": "^6.2.3",
    "typescript": "4.8.3"
  },
  "prettier": {
    "singleQuote": false,
    "semi": true,
    "trailingComma": "all"
